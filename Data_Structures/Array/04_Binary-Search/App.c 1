#include<iostream>
#include<cmath>
using namespace std;
int arr[10]={10,1,53,6,23,1,15,5,7};

int bS(int l,int h,int key){
while(l<=h){ // If the l is greater than the right then return the -1 because we have gone through the every digit
    int m =floor(( l+h)/2);
    if(arr[m]== key){
        return m;
    }else if(key<arr[m]){
        h= m-1;
    }else{
        l = m+1;
    }
}
return -1;
}


int main(){
    return 0;
}