#include <iostream>
using namespace std;


int linearSearch(int arr[],int key,int length){
    for(int i =0;i<=length-1;i++){
        if(key == arr[i]){     
            int temp = arr[i];//The swapping is kind of optimization this is know as the move to front strategy
             arr[i]  =arr[0];//In this method we will swap the element to the 0th position 
             arr[0] = temp;//
             return i;
        }
    }
    return -1;
};

int main(){
    int arr[7] = {100,13,136,352,147,1241,68};
    return 0;
}
